{
  "name": "my-jira",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">= 12.6.0"
  },
  "dependencies": {
    "@emotion/core": "^11.0.0",
    "@emotion/react": "^11.1.4",
    "@emotion/styled": "^11.0.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "antd": "^4.10.3",
    "craco": "^0.0.3",
    "craco-less": "^1.17.1",
    "jira-dev-tool": "^1.7.8",
    "qs": "^6.9.6",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.1",
    "typescript": "^4.0.3",
    "web-vitals": "^0.2.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/qs": "^6.9.5",
    "eslint-config-prettier": "^7.1.0",
    "husky": "^4.3.7",
    "lint-staged": "^10.5.3",
    "prettier": "^2.1.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "prettier"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,css,md,ts,tsx}": "prettier --write",
    "*.{js,css,md}": "prettier --write"
  },
  "vsc-organize-imports": {
    "orderSpecifiers": true,
    "orderSpecifiersAsSingleLine": true,
    "baseUrl": "src",
    "emptyLinesAfterImports": 0,
    "emptyLinesBetweenFilledGroups": 1,
    "groups": [
      {
        "groups": [
          "global"
        ],
        "sortBy": "path",
        "emptyLines": true
      },
      {
        "groups": [
          "absolute"
        ],
        "sortBy": "path",
        "emptyLines": true
      },
      {
        "groups": [
          "relative"
        ],
        "sortBy": "path",
        "emptyLines": true
      },
      {
        "groups": [
          "globalDirect",
          "absoluteDirect",
          "relativeDirect"
        ],
        "sortBy": "path",
        "emptyLines": true
      }
    ]
  }
}
